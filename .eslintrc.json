{
  "extends": [
    "airbnb",
    "prettier",
    "plugin:@typescript-eslint/eslint-recommended"
  ],
  "rules": {
    "react/prefer-stateless-function": 0,
    "react/jsx-filename-extension": 0,
    "react/jsx-one-expression-per-line": 0,
    "quotes": [
      2,
      "double",
      {
        "avoidEscape": true
      }
    ]
  },
  "env": {
    "browser": true
  }
}